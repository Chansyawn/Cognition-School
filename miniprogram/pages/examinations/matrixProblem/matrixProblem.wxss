.page{
  background-color: #eeeeee;
}

.main {
  padding: 0 40rpx;
}

.row {
  display: flex;
  justify-content: space-between;
  margin: 10px 0
}

.cell {
  position: relative;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 10rpx;
  border-radius: 16px;
  background-color: #ffffff;
}

.cell:before{
  content: '';
  padding-top: 100%;
}

.cell-content {
  width: 200rpx;
  height: 200rpx;
}