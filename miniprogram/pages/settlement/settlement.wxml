<view class="page">
  <custom-nav-bar navigateBackArea="{{false}}">成绩单</custom-nav-bar>
  <view class="main">
    <view class="report-title">PASS 测试报告</view>
    <view class="header-information">
      <view class="user-information-container">
        <view class="user-information">姓名: {{name}}</view>
        <view class="user-information">年龄: {{age}}</view>
        <view class="user-information">得分: {{totalScore}}</view>
      </view>
      <image class="score-image" src="/assets/images/tag.svg" mode="widthFix"></image>
    </view>
    <ec-canvas wx:if="{{true}}" class="data-chart" ec="{{ ecBar }}"></ec-canvas>
    <view class="finish-date"><text>{{finishDate + '\n' + finishTime}}</text></view>
  </view>
  <custom-tab-bar></custom-tab-bar>
</view>